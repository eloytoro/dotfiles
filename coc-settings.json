{
  "signature.hideOnTextChange": true,
  "suggest.autoTrigger": "always",
  "hover.target":"float",
  "coc.preferences.jumpCommand": "tab drop",
  "coc.preferences.listOfWorkspaceEdit": "none",
  "coc.source.emoji.filetypes": ["markdown", "gitcommit"],
  "typescript.format.enabled": false,
  "tsserver.enableJavascript": true,
  "eslint.packageManager": "yarn",
  "eslint.autoFixOnSave": true,
  "git.showCommitInFloating": true,
  "snippets.userSnippetsDirectory": "$HOME/dotfiles/snippets"
}
